{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/root.json",
    "type": "object",
    "title": "The Root Schema",
    "required": [
        "jobs"
    ],
    "properties": {
        "jobs": {
            "$id": "#/properties/jobs",
            "type": "array",
            "title": "The Jobs Schema",
            "items": {
                "$id": "#/properties/jobs/items",
                "type": "object",
                "title": "The Items Schema",
                "required": [
                    "job_id",
                    "type",
                    "status",
                    "owner",
                    "narrative",
                    "app",
                    "client_groups"
                ],
                "properties": {
                    "job_id": {
                        "$id": "#/properties/jobs/items/properties/job_id",
                        "type": "string",
                        "title": "The Job_id Schema",
                        "default": "",
                        "examples": [
                            "5d769018aa5a4d298c5dc97a"
                        ],
                        "pattern": "^(.*)$"
                    },
                    "type": {
                        "$id": "#/properties/jobs/items/properties/type",
                        "type": "string",
                        "title": "The Type Schema",
                        "default": "",
                        "examples": [
                            "narrative"
                        ],
                        "pattern": "^(.*)$"
                    },
                    "status": {
                        "$id": "#/properties/jobs/items/properties/status",
                        "type": "string",
                        "enum": [
                            "queued",
                            "running",
                            "completed",
                            "errored-queued",
                            "errored-running",
                            "canceled-queued",
                            "canceled-running"
                        ],
                        "title": "The Status Schema",
                        "default": "",
                        "examples": [
                            "completed"
                        ],
                        "pattern": "^(.*)$"
                    },
                    "owner": {
                        "$id": "#/properties/jobs/items/properties/owner",
                        "type": "object",
                        "title": "The Owner Schema",
                        "required": [
                            "username",
                            "realname"
                        ],
                        "properties": {
                            "username": {
                                "$id": "#/properties/jobs/items/properties/owner/properties/username",
                                "type": "string",
                                "title": "The Username Schema",
                                "default": "",
                                "examples": [
                                    "eapearson"
                                ],
                                "pattern": "^(.*)$"
                            },
                            "realname": {
                                "$id": "#/properties/jobs/items/properties/owner/properties/realname",
                                "type": "string",
                                "title": "The Realname Schema",
                                "default": "",
                                "examples": [
                                    "Erik A. Pearson"
                                ],
                                "pattern": "^(.*)$"
                            }
                        }
                    },
                    "narrative": {
                        "$id": "#/properties/jobs/items/properties/narrative",
                        "type": "object",
                        "title": "The Narrative Schema",
                        "required": [
                            "workspace_id",
                            "workspace_name",
                            "title"
                        ],
                        "properties": {
                            "workspace_id": {
                                "$id": "#/properties/jobs/items/properties/narrative/properties/workspace_id",
                                "type": "integer",
                                "title": "The Workspace_id Schema",
                                "default": "",
                                "examples": [
                                    43676
                                ],
                                "pattern": "^(.*)$"
                            },
                            "workspace_name": {
                                "$id": "#/properties/jobs/items/properties/narrative/properties/workspace_name",
                                "type": "string",
                                "title": "The Workspace_name Schema",
                                "default": "",
                                "examples": [
                                    "eapearson:narrative_1564775265770"
                                ],
                                "pattern": "^(.*)$"
                            },
                            "title": {
                                "$id": "#/properties/jobs/items/properties/narrative/properties/title",
                                "type": "string",
                                "title": "The Title Schema",
                                "default": "",
                                "examples": [
                                    "Another job browser testsx"
                                ],
                                "pattern": "^(.*)$"
                            }
                        }
                    },
                    "app": {
                        "$id": "#/properties/jobs/items/properties/app",
                        "type": "object",
                        "title": "The App Schema",
                        "required": [
                            "module_name",
                            "function_name",
                            "title"
                        ],
                        "properties": {
                            "module_name": {
                                "$id": "#/properties/jobs/items/properties/app/properties/module_name",
                                "type": "string",
                                "title": "The Module_name Schema",
                                "default": "",
                                "examples": [
                                    "ProkkaAnnotation"
                                ],
                                "pattern": "^(.*)$"
                            },
                            "function_name": {
                                "$id": "#/properties/jobs/items/properties/app/properties/function_name",
                                "type": "string",
                                "title": "The Function_name Schema",
                                "default": "",
                                "examples": [
                                    "annotate_contigs"
                                ],
                                "pattern": "^(.*)$"
                            },
                            "title": {
                                "$id": "#/properties/jobs/items/properties/app/properties/title",
                                "type": "string",
                                "title": "The Title Schema",
                                "default": "",
                                "examples": [
                                    "Annotate Assembly and ReAnnotate Genomes with Prokka v1.12"
                                ],
                                "pattern": "^(.*)$"
                            }
                        }
                    },
                    "client_groups": {
                        "$id": "#/properties/jobs/items/properties/client_groups",
                        "type": "array",
                        "title": "The Client_groups Schema",
                        "items": {
                            "$id": "#/properties/jobs/items/properties/client_groups/items",
                            "type": "string",
                            "title": "The Items Schema",
                            "default": "",
                            "examples": [
                                "njs"
                            ],
                            "pattern": "^(.*)$"
                        }
                    }
                }
            }
        }
    }
}